{
  "name": "main",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --host 127.0.0.1",
    "start:production": "ng serve --hmr --configuration production --watch --port 4210",
    "build": "ng build --configuration production",
    "build:watch": "ng build --watch --configuration production",
    "build:generate-stats": "ng build --configuration production --stats-json && webpack-bundle-analyzer ../server/public/stats.json",
    "build:analyze-stats": "yarn run build:generate-stats && webpack-bundle-analyzer ../server/public/stats.json",
    "optimize-icon-svg": "svgo --config .svgo.config.js -f ./src/assets/icons/ -r -o ./src/assets/icons/.tmp",
    "generate-icon-ts": "svg-to-ts-constants",
    "generate-icons": "yarn run optimize-icon-svg && yarn run generate-icon-ts",
    "generate-apollo": "graphql-codegen",
    "generate-apollo:start": "graphql-codegen --watch",
    "generate-icon-data": "scripts/generate-icon-data.js",
    "generate-icon-rst": "scripts/generate-icon-rst.js",
    "generate-docs-rst": "yarn run generate-icon-data && yarn run generate-icon-rst",
    "lint": "ng lint"
  },
  "private": true,
  "resolutions": {
    "error-stack-parser": "2.0.6"
  },
  "dependencies": {
    "@angular-devkit/schematics": "20.0.5",
    "@angular/animations": "^20.0.6",
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "@apollo/client": "^3.13.1",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/component": "^19.0.1",
    "@ngx-formly/core": "^6.3.12",
    "@ngx-formly/ng-zorro-antd": "^6.3.12",
    "apollo-angular": "^10.0.3",
    "ng-zorro-antd": "19.1.0",
    "ngx-cookie-service": "19.1.2",
    "ngx-ui-scroll": "3.2.0",
    "rxjs": "^7.8.1",
    "rxjs-etc": "^10.6.2",
    "rxjs-spy": "^8.0.2",
    "ts-enum-util": "^4.0.2",
    "ts-mixin-extended": "^1.1.1",
    "vscroll": "1.6.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.5",
    "@angular-devkit/core": "^20.0.5",
    "@angular-eslint/builder": "19.2.0",
    "@angular-eslint/eslint-plugin": "19.2.0",
    "@angular-eslint/eslint-plugin-template": "19.2.0",
    "@angular-eslint/schematics": "19.2.0",
    "@angular-eslint/template-parser": "19.2.0",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "20.0.6",
    "@ctrl/tinycolor": "^4.1.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "directory-tree": "^3.5.1",
    "eslint": "8.57.0",
    "graphql": "^16.8.1",
    "mustache": "^4.2.0",
    "ngx-json-viewer": "^3.0.2",
    "prettier": "^3.2.4",
    "svg-to-ts": "^12.0.0",
    "svgo": "^3.0.2",
    "ts-node": "~10.9.1",
    "tslib": "^2.3.1",
    "tslint": "~6.1.0",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "overrides": {
    "rxjs-spy": {
      "error-stack-parser": "2.0.6"
    }
  }
}
