# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
tab_width = 2
indent_style = space
indent_size = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.ts]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.less]
quote_type = double
insert_final_newline = true
trim_trailing_whitespace = true
