input GeneFilter {
  id: IDInput
  title: StringInput 
  clickCount: IntInput
  floatValue: FloatInput 
  description: StringInput
  operation: Enum{AND,OR}
  subFilters: [NoteFilter]
}

input StringInput  {
  operator: Enum(eq,  ne, like, )
  operand: value
}






Cases:
 supplied multiple fields
  title contains 'foo' AND floatValue = 3 AND (clickCount > 2 or description contains 'bar')

 supplied one field
  title contains 'foo' AND (clickCount > 2 or description contains 'bar')

 supplied no fields
  (title contains 'foo' OR floatValue = 3) AND (clickCount > 2 or description contains 'bar')